#ifndef PARAM_H_
#define PARAM_H_

struct Temp{
	float* w1;	float* w2;	float* w3;	float* w4;
	float* x1;	float* x2;	float* x3;	float* x4;
	float* y1;	float* y2;	float* y3;	float* y4;
	float* z1;	float* z2;	float* z3;	float* z4;
};

struct Hydro_info{
	float* x;  	//[4]
 	float* y;	//[4]
 	float* phase_err;	//[4*4]
 	float* lstsqr;		//[3*4]
};


#endif /* PARAM_H_ */
